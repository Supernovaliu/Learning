class VariableTest{
	public static void main(String[] args){
		int myAge = 12;
		System.out.println(myAge);

		int myNumber;
		myNumber = 1001;

		System.out.println(myNumber);

		float f1 = 12.3F;
		System.out.println(f1);
		char c1 = 'a';
		System.out.println(c1);

		char c5 = '\t';
		System.out.print("Hello"+c5);
		System.out.println("World");

	}

} 
