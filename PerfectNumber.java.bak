class PerfectNumber{
	public static void main(String[] args){
		int a = 0;
		int b = 0;
		for (int i=1;i<=1000 ;i++ )
		{
              for (int j=1;j<i ;j++ )
              {
				  
                  if (i%j==0)
                  {
					  b+=j;
					  
                  }
				   else if(b==i){
						System.out.println(i);
					  }
              }
		}
	}
}
